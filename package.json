{
  "name": "xhb",
  "version": "1.0.2",
  "description": "Provides ability to read and modify xhb files used by HomeBank",
  "main": "build/index.js",
  "author": {
    "name": "George Kotchlamazashvili <georgedot@gmail.com>",
    "email": "georgedot@gmail.com"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "xhb",
    "file",
    "parser",
    "parse",
    "serialize",
    "serializer",
    "read",
    "reader",
    "write",
    "writer",
    "home",
    "bank",
    "homebank",
    "ts",
    "typescript"
  ],
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hertzg/node-xhb.git"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly --listEmittedFiles",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --ignore **/__tests__ --verbose",
    "prettier": "prettier",
    "test": "jest"
  },
  "dependencies": {
    "printj": "^1.2.2",
    "xml-parser": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@types/xml-parser": "^1.2.29",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.2"
  }
}
